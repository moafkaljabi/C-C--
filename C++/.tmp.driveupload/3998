// encapsulation

#include <iostream>
using namespace std;

class employee
{
private:
    int salary;

public:
    void setSalary(int s)
    {
        salary = s;
    }
    int getsalary()
    {
        return salary;
    }
};

int main()
{
    employee empObj1;
    empObj1.setSalary(5000);

    cout << empObj1.setSalary << "\n";

    return 0;
}