// C++ Multilevel Inheritance

#include <iostream>
using namespace std;

class myClass
{
public:
    void myFunction()
    {
        cout << "Printing some stuff.\n";
    }
};

class myChild : public myClass
{
public:
};

class myGrandChild : public myChild
{
public:
};

int main()
{
    myGrandChild theObj1;
    theObj1.myFunction();
    return 0;
}
