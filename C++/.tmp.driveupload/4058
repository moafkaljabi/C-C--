// Default Values for Parameters

#include <iostream>
#include <math.h>
#include <cmath>
using namespace std;

int func1(int a, int b = 10)
{
    int result;
    result = a + b;
    return (result);
}

int main()
{
    int a = 100;
    int b = 200;
    int result;
    result = func1(a, b);
    cout << result << endl;
    result = func1(a);
    cout << result << endl;

    return 0;
}