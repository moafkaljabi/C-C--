// Inheritance

#include <iostream>
using namespace std;

class Vehicle
{
public:
    string brand = "Ford";

    void color()
    {
        cout << "Blue ";
    }
};

class Car : public Vehicle
{
public:
    string name = "Mustang";
};

int main()
{
    Car myCar;
    myCar.color();
    cout << myCar.brand + " " + myCar.name << "\n\n";

    return 0;
}
