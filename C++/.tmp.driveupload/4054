#include <iostream>
using namespace std;

struct car
{
    string Brand;
    string Name;
    int year;
};

int main()
{

    car myCar_1;

    myCar_1.Brand = "Cadillac";
    myCar_1.Name = "Escalade";
    myCar_1.year = 2022;

    car myCar_2;
    myCar_2.Brand = "GMC";
    myCar_2.Name = "Sierra";
    myCar_2.year = 2023;

    car hisCar_1;
    hisCar_1.Brand = "Rolls Royce";
    hisCar_1.Name = "Cullinan";
    hisCar_1.year = 2020;

    cout << "\n";
    cout << myCar_1.Brand << " " << myCar_1.Name << " " << myCar_1.year << " \n";
    cout << myCar_2.Brand << " " << myCar_2.Name << " " << myCar_2.year << " \n";
    cout << hisCar_1.Brand << " " << hisCar_1.Name << " " << hisCar_1.year << "\n\n";

    return 0;
}